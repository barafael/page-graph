<script>
window.onload = function () {
    var videos = document.getElementsByTagName('video')
    if (videos && videos.length > 0) {
        for (var i = 0; i< videos.length; i++) {
            videos[i].loop = false
        }
    }
}
</script>
<script>
function loadHubSpotAnalytics() {
	!function(e, t) {
		if (!document.getElementById(e)) {
			var c = document.createElement("script");
			c.src = "https://js.hs-analytics.net/analytics/1608375600000/6934192.js",
				c.type = "text/javascript",
				c.id = e;
			var n = document.getElementsByTagName("script")[0];
			n.parentNode.insertBefore(c, n)
		}
	}("hs-analytics");
}
function loadHubSpotLiveChat() {
	!function(t, e, r) {
		if (!document.getElementById(t)) {
			var n = document.createElement("script");
			for (var a in n.src = "https://js.usemessages.com/conversations-embed.js", n.type = "text/javascript", n.id = t, r)
				r.hasOwnProperty(a) && n.setAttribute(a, r[a]);
			var i = document.getElementsByTagName("script")[0];
			i.parentNode.insertBefore(n, i)
		}
	}("hubspot-messages-loader", 0, {
		"data-loader": "hs-scriptloader",
		"data-hsjs-portal": 6934192,
		"data-hsjs-env": "prod"
	});
}
function loadHubSpotLeadIn() {
	!function(t, e, r) {
		if (!document.getElementById(t)) {
			var n = document.createElement("script");
			for (var a in n.src = "https://js.hsleadflows.net/leadflows.js", n.type = "text/javascript", n.id = t, r)
				r.hasOwnProperty(a) && n.setAttribute(a, r[a]);
			var i = document.getElementsByTagName("script")[0];
			i.parentNode.insertBefore(n, i)
		}
	}("LeadFlows-6934192", 0, {
		"crossorigin": "anonymous",
		"data-leadin-portal-id": 6934192,
		"data-leadin-env": "prod",
		"data-loader": "hs-scriptloader",
		"data-hsjs-portal": 6934192,
		"data-hsjs-env": "prod"
	});
}
function loadHubSpotForms() {
	!function(t, e, r) {
		if (!document.getElementById(t)) {
			var n = document.createElement("script");
			for (var a in n.src = "https://js.hscollectedforms.net/collectedforms.js", n.type = "text/javascript", n.id = t, r)
				r.hasOwnProperty(a) && n.setAttribute(a, r[a]);
			var i = document.getElementsByTagName("script")[0];
			i.parentNode.insertBefore(n, i)
		}
	}("CollectedForms-6934192", 0, {
		"crossorigin": "anonymous",
		"data-leadin-portal-id": 6934192,
		"data-leadin-env": "prod",
		"data-loader": "hs-scriptloader",
		"data-hsjs-portal": 6934192,
		"data-hsjs-env": "prod"
	});
}
function loadGoogleAnalytics() {
	let firstScript = document.getElementsByTagName("script")[0]
	let tagManagerId = 'google_gtagjs-js'
	let tagManager = document.getElementById(tagManagerId);	
	if (!tagManager) {
		tagManager = document.createElement('script')
		tagManager.setAttribute("id", tagManagerId)
		tagManager.setAttribute("type", "text/javascript")
		tagManager.setAttribute("src", "https://www.googletagmanager.com/gtag/js?id=UA-176005117-1")
		tagManager.setAttribute("async", "")
		firstScript.parentNode.insertBefore(tagManager, firstScript)
	}

	let tagManagerOptionsId = 'google_gtagjs-js-after'
	let tagManagerOptions = document.getElementById(tagManagerOptionsId);	
	if (!tagManagerOptions) {
		tagManagerOptions = document.createElement('script')
		tagManagerOptions.setAttribute("id", tagManagerId)
		tagManagerOptions.setAttribute("type", "text/javascript")
		tagManagerOptions.text = `<!-- Global site tag (gtag.js) - Google Analytics -->
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  gtag('config', 'UA-176005117-1');
`
		firstScript.parentNode.insertBefore(tagManagerOptions, firstScript)
	}
}
function updateCookieConsentStatus(data) {
	if (data['cookieconsent_status_functional'] === "allow" ) {
		loadHubSpotForms()
		loadHubSpotLeadIn()
		loadHubSpotLiveChat()
	}
	else {
	}


	if (data['cookieconsent_status_stats'] === "allow" ) {
		loadHubSpotAnalytics()
		loadGoogleAnalytics()
	}
}
function initCookieStatus() {
    var data = {};
document.cookie.split(/\s*;\s*/).forEach(function(pair) {
  pair = pair.split(/\s*=\s*/);
  data[pair[0]] = pair.splice(1).join('=');
});
	updateCookieConsentStatus(data)
}
window.cookieStatusChanged = function(data) {
	updateCookieConsentStatus(data)
}
initCookieStatus()
